{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "UI Export Schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "private": {
      "type": "boolean"
    },
    "extension": {
      "type": "string"
    },
    "scripts": {
      "type": "object",
      "properties": {
        "build": {
          "type": "string"
        },
        "dev": {
          "type": "string"
        },
        "format": {
          "type": "string"
        },
        "lint": {
          "type": "string"
        }
      },
      "required": ["build", "dev", "format", "lint"]
    }
  },
  "required": ["name", "private", "extension"]
}
